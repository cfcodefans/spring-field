{
    "compilerOptions": {
        "target": "ESNext",
        "module": "ESNext",
        "outDir": ".",
        "strict": false,
        "esModuleInterop": true,
        "skipLibCheck": true,
        "forceConsistentCasingInFileNames": true
    },
    "include": [
        "*.ts"
    ],
    "exclude": [
        "*.js"
    ]
}